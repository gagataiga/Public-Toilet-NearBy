  "main": "index.js",
  "scripts": {
    "build": "npm install && npx typescript && npm run migration && npm run seed",
    "start": "node ./dist/server/index.js",
    "env-copy": "cp ./.env ./dist/.env",
    "migration": "knex migrate:latest",
    "migration-production": "knex migrate:latest --knexfile ./dist/knexfile.js",
    "seed": "knex seed:run",
    "dev": "nodemon --exec ts-node ./server/index.ts",
    "client": "cd client && npm run start",
    "client-test": "cd client && npm run test",
    "backend-test": "mocha --require ts-node/register test/**/*.ts"
  },
