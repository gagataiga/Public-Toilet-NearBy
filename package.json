    "@types/node": "^20.8.0",
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.11.2",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.8",
    "@types/node-cache": "^4.2.5",
